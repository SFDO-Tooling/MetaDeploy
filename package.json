    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.3.1",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "coveralls": "^3.0.2",
    "css-loader": "^1.0.0",
    "eslint": "^5.7.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-flowtype": "^3.0.0",
    "eslint-plugin-flowtype-errors": "^3.6.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "fetch-mock": "^7.2.0",
    "file-loader": "^2.0.0",
    "flow-bin": "^0.83.0",
    "flow-types": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "jest-dom": "^2.1.0",
    "jest-junit": "^5.2.0",
    "lint-staged": "^7.3.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.9.4",
    "npm-run-all": "^4.1.3",
    "nyc": "^13.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.3",
    "react-testing-library": "^5.2.1",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "size-plugin": "^1.0.1",
    "stylelint": "^9.6.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.3.2",
    "terser-webpack-plugin": "^1.1.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.23.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@salesforce-ux/design-system": "^2.7.4",
    "@salesforce/design-system-react": "^0.8.28",
    "classnames": "^2.2.6",
    "js-cookie": "^2.2.0",
    "money-clip": "^3.0.0",
    "raven-js": "^3.27.0",
    "react": "^16.6.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.6.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-persist-middleware": "^1.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "sockette": "^2.0.0",
    "whatwg-fetch": "^3.0.0"
  },
  "scripts": {
    "webpack:serve": "webpack-dev-server --config webpack.dev.js",
    "django:serve": "python manage.py runserver",
    "worker:serve": "python manage.py rqworker default short",
    "scheduler:serve": "python manage.py rqscheduler default short",
    "rq:serve": "run-p worker:serve scheduler:serve",
    "serve": "run-p django:serve webpack:serve rq:serve",
    "prettier": "prettier --write '*.js' '.*.js' 'src/js/*.js' 'src/js/**/*.js' 'test/js/*.js' 'test/js/**/*.js' 'src/sass/**/*.scss'",
    "eslint": "eslint '*.js' 'src/js/*.js' 'src/js/**/*.js' 'test/js/*.js' 'test/js/**/*.js'",
    "stylelint": "stylelint 'src/sass/**/*.scss'",
    "flake8": "flake8 manage.py metadeploy/ config/",
    "lint": "npm-run-all -p prettier flake8 -p eslint stylelint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "JEST_JUNIT_OUTPUT='./coverage/junit.xml' jest --ci --runInBand --reporters=default --reporters=jest-junit",
    "test:report-coverage": "cat ./coverage/lcov.info | coveralls",
    "test:check-coverage": "nyc check-coverage --temp-directory coverage",
    "pytest": "pytest",
    "pytest:report-coverage": "$VIRTUAL_ENV/bin/coveralls",
    "pytest:check-coverage": "coverage3 report --fail-under=100",
    "build": "webpack --config webpack.dev.js",
    "prod": "webpack --config webpack.prod.js",
    "heroku-postbuild": "yarn prod"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "./bin/prepare-commit-msg"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint",
      "git add"
    ],
    "*.scss": [
      "prettier --write",
      "stylelint",
      "git add"
    ],
    "*.py": [
      "flake8",
      "git add"
    ]
  }
}
