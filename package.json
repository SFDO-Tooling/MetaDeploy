{
  "name": "metadeploy",
  "description": "Web-based tool for installing Salesforce products",
  "version": "0.1.0",
  "private": true,
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/SFDO-Tooling/MetaDeploy.git"
  },
  "engines": {
    "node": "12.13.0",
    "yarn": "1.22.4"
  },
  "scripts": {
    "webpack:serve": "webpack-dev-server --config webpack.dev.js",
    "django:serve": "python manage.py runserver 0.0.0.0:${PORT:-8000}",
    "worker:serve": "python manage.py rqworker default short",
    "scheduler:serve": "python manage.py rqscheduler --queue short",
    "rq:serve": "run-p worker:serve scheduler:serve",
    "serve": "run-p django:serve webpack:serve rq:serve",
    "prettier:js": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "prettier:other": "prettier --write '**/*.{json,md,yml}'",
    "eslint": "yarn eslint:nofix --fix",
    "eslint:nofix": "eslint '*.js' src/js test/js --ext .js,.ts,.jsx,.tsx",
    "tsc": "tsc",
    "flake8": "flake8 manage.py metadeploy/ config/",
    "isort": "isort manage.py metadeploy/ config/",
    "black": "black manage.py metadeploy/ config/",
    "lint": "run-p lint:py lint:js lint:sass prettier:other",
    "lint:nofix": "run-p flake8 eslint:nofix lint:sass:nofix tsc",
    "lint:js": "run-s prettier:js eslint tsc",
    "lint:py": "run-s isort black flake8",
    "lint:sass:nofix": "stylelint '**/*.scss'",
    "lint:sass": "yarn lint:sass:nofix --fix",
    "test:js": "jest",
    "test:js:watch": "jest --watchAll",
    "test:js:coverage": "JEST_JUNIT_OUTPUT_DIR='./coverage' jest --ci --runInBand --reporters=default --reporters=jest-junit",
    "test:js:report-coverage": "cat ./coverage/lcov.info | coveralls",
    "test:js:check-coverage": "nyc check-coverage --temp-directory coverage",
    "test:py": "pytest",
    "test:py:report-coverage": "$VIRTUAL_ENV/bin/coveralls",
    "test:py:check-coverage": "coverage report --fail-under=100",
    "test:py:integration": "pytest -m 'integration'",
    "test": "run-s test:js test:py",
    "build": "webpack --config webpack.dev.js",
    "prod": "webpack --config webpack.prod.js",
    "heroku-postbuild": "yarn prod"
  },
  "dependencies": {
    "@salesforce-ux/design-system": "2.12.2",
    "@salesforce/design-system-react": "0.10.20",
    "@sentry/browser": "^5.20.0",
    "classnames": "^2.2.6",
    "i18next": "^19.6.2",
    "i18next-browser-languagedetector": "^5.0.0",
    "i18next-xhr-backend": "^3.2.2",
    "js-cookie": "^2.2.1",
    "react": "^16.13.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.13.1",
    "react-fns": "^1.4.0",
    "react-i18next": "^11.7.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "sockette": "^2.0.6",
    "uuid": "^8.2.0",
    "whatwg-fetch": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^10.4.7",
    "@testing-library/react-hooks": "^3.3.0",
    "@types/classnames": "^2.2.10",
    "@types/core-js": "^2.5.3",
    "@types/js-cookie": "^2.2.6",
    "@types/react": "^16.9.43",
    "@types/react-document-title": "^2.0.4",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/redux-logger": "^3.0.8",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "autoprefixer": "^9.8.5",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.6.5",
    "coveralls": "^3.1.0",
    "css-loader": "^3.6.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "fetch-mock": "^9.10.4",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "i18next-scanner-webpack": "^0.5.1",
    "jest": "^26.1.0",
    "jest-junit": "^11.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "redux-mock-store": "^1.5.4",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.2",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.18.0",
    "svg-inline-loader": "^0.8.2",
    "typescript": "^3.9.7",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.0.9"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "./bin/prepare-commit-msg"
    }
  }
}
