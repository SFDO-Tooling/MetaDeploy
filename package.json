    "@types/react-router-dom": "^5.3.3",
    "@types/redux-logger": "^3.0.9",
    "@types/redux-mock-store": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "autoprefixer": "^10.4.7",
    "babel-jest": "^28.1.2",
    "babel-loader": "^8.2.5",
    "core-js": "^3.23.3",
    "coveralls": "^3.1.1",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.2.4",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-mdx": "^2.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-storybook": "^0.5.13",
    "fetch-mock": "^9.11.0",
    "html-webpack-plugin": "^5.5.0",
    "i18next-scanner-webpack": "0.8.2",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "jest-junit": "^14.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "node-fetch": "^2.6.7",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.0",
    "prettier": "^2.7.1",
    "redux-mock-store": "^1.5.4",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^8.0.0",
    "stylelint-config-standard-scss": "^4.0.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-merge": "^5.8.0"
  },
  "resolutions": {
    "@storybook/**/ansi-regex": "^5.0.1",
    "@storybook/**/glob-parent": "^5.1.2",
    "@storybook/**/trim-newlines": "^3.0.1",
    "i18next-scanner-webpack/**/glob-parent": "^5.1.2",
    "trim": "^0.0.3",
    "unset-value": "^2.0.1"
  },
  "yarn-upgrade-all": {
    "ignore": [
      "i18next-scanner-webpack",
      "node-fetch",
      "react-router-dom"
    ]
  },
  "storybook-deployer": {
    "gitUsername": "Storybook Deployer Bot",
    "gitEmail": "dev-ops@oddbird.net",
    "commitMessage": "Deploy Storybook [skip ci]"
  }
}
